local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function kAacZIWLoKHEGlnHS(data) m=string.sub(data, 0, 55) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


-- Advanced Invisibility Script (For Educational Purposes Only)

-- Variables
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local isInvisible = false
local originalPosition = nil

-- Function to toggle invisibility
local function toggleInvisibility()
    if not character then return end
    local humanoid = character:FindFirstChildOfClass(kAacZIWLoKHEGlnHS('JaGlwtvmxeZLIbIyTWKZxJQBqNMMOXdPhuuuKaFvcbISqlnEEIfijgXSHVtYW5vaWQ='))
    if not humanoid then return end

    if isInvisible then
        -- Restore visibility
        pcall(function()
            character.Parent = workspace -- Bring character back to the world
            character:SetPrimaryPartCFrame(originalPosition) -- Move back to original position
            for _, part in pairs(character:GetChildren()) do
                if part:IsA(kAacZIWLoKHEGlnHS('RmaAqDVMrJimgoIjyqGbdBtUbdiEqTSUBLiBanevVLqNgJAcgqZmdwjQmFzZVBhcnQ=')) then
                    part.Transparency = 0
                end
            end
            humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.Viewer
            isInvisible = false
        end)
    else
        -- Make invisible
        pcall(function()
            originalPosition = character:GetPrimaryPartCFrame() -- Save current position
            character.Parent = nil -- Remove character from workspace
            humanoid.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
            isInvisible = true
        end)
    end
end

-- Create GUI for toggling invisibility
local screenGui = Instance.new(kAacZIWLoKHEGlnHS('bzcuATxXuBxNBtWqvjRoRhFkaUKNLsvctMrtHQLOESkFidxPVuGBFrQU2NyZWVuR3Vp'))
local toggleButton = Instance.new(kAacZIWLoKHEGlnHS('eeGizELBktAhbspbUDyxQXCjLCKYxCZgAPJUxjUBjvZAumYChjwFMeeVGV4dEJ1dHRvbg=='))

screenGui.Name = kAacZIWLoKHEGlnHS('bUnwvZdhWGBfHWZHwZdzxkXDxLYPqsZIklxXZoSwYvsgMVBTrSdFFIxSW52aXNpYmlsaXR5R1VJ')
screenGui.Parent = player:WaitForChild(kAacZIWLoKHEGlnHS('hKwEWUaQxKiEnFpmqkktFRYitxTUekATpClqRUFJExDRcBygiWJJMWAUGxheWVyR3Vp'))
screenGui.ResetOnSpawn = false -- Persistent GUI

toggleButton.Size = UDim2.new(0, 150, 0, 50)
toggleButton.Position = UDim2.new(0.5, -75, 0.5, -25)
toggleButton.Text = kAacZIWLoKHEGlnHS('tGqChzfvMiJIYxTOWpovIoJQKCmdVClUNLxvSTlyQMKbBOXRJVadwrcVG9nZ2xlIEludmlzaWJpbGl0eQ==')
toggleButton.BackgroundColor3 = Color3.new(0.3, 0.3, 0.3)
toggleButton.TextColor3 = Color3.new(1, 1, 1)
toggleButton.Parent = screenGui

-- Connect button click to toggle invisibility
toggleButton.MouseButton1Click:Connect(toggleInvisibility)    
